<div class="flyntComponent" is="flynt-list-social">
  <div class="container">
    <div class="socials">
      <ul class="list">
        {% for panel in repeaterPanels %}
          <li class="panel">
            <a href="{{ panel.panelLink.url }}" target="_blank" rel="noreferrer noopener" type="button">
              {{ panel.panelLink.title|e }}
            </a>
          </li>
        {% endfor %}
      </ul>
    </div>
  </div>
</div>
