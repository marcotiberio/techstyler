<div class="flyntComponent" id="footnote" is="flynt-block-footnotes">
  <div class="container boxed">
    {# <div class="content">
      {{ contentHtml|e('wp_kses_post') }}
    </div> #}
    <div class="content grid8">
      <ul class="footnotes">
        {% for footnote in footnotesPanels %}
          <li class="footnote" id="content-{{ loop.index }}">
            <span><sup class="left">{{ footnote.footnoteNumber }}</sup></span>
            <span class="right">
              <a href="{{ footnote.footnoteContent.url }}" target="_blank" rel="noreferrer noopener" type="button">
                {{ footnote.footnoteContent.title|e }}
              </a>
            </span>
          </li>
        {% endfor %}
      </ul>
    </div>
  </div>
</div>
